{% extends "base.html.twig" %}

{% block title %}
Create a new record | {{ parent() }}
{% endblock %}

{% block body %}
    <h1>{{ title }}</h1>

    <div>
        <ul>
            {% for track in tracks %}
                <li>
                    {{ track.song }} - {{ track.artist }}
                </li>
            {% endfor %}
        </ul>
    </div>
{% endblock %}